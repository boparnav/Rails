<h1> this page shows new books</h1>


<%= form_for @book do |f| %>
  <%= f.label   :title %>

  <%= f.text_field :title  %>

  <br><br>

  <%= f.label :author %>

  <%= f.text_field :author  %>

  <br><br>
  <%= f.label :description %>

  <%= f.text_field :description  %>

  <br><br><br>

  <%= f.submit %>


<% end %>